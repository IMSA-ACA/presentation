  body
    nav.navbar.navbar-default.navbar-static-top
      .container
        .navbar-header
          a.navbar-brand(href="./") ACA
          button.navbar-toggle.collapsed(data-toggle="collapse", data-target=".navbar-collapse")
            span.sr-only Toggle navigation
            span.icon-bar 
            span.icon-bar
            span.icon-bar
        .navbar-collapse.collapse
          ul.nav.navbar-nav.navbar-right
            li
              a(href="./about") About
            li
              a(href="./events") Events
            li.dropdown
              a.dropdown-toggle(data-toggle="dropdown" href="./projects") Projects 
                span.caret
              ul.dropdown-menu
                li
                  a(href="#") Gaming Tunnel
                li
                  a(href="#") Member Projects
                li
                  a(href="#") Organizational Projects
                li.divider
                li
                  a(href="#") More
    block content
    block foot
